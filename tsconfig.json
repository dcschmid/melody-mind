{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@utils/*": ["src/utils/*"],
      "@constants/*": ["src/constants/*"],
      "@i18n/*": ["src/i18n/*"],
      "@content/*": ["src/content/*"],
      "@types/*": ["src/types/*"],
      "@data/*": ["src/data/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strict": true,
    "types": ["astro/client"]
  }
}
